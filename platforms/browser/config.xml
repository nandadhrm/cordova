<?xml version='1.0' encoding='utf-8'?>
<widget id="org.apache.cordova.EzTicket" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EzTicket</name>
    <description>
        A movie ticket reservation app created by very determined college students.
    </description>
    <author email="works.ananda@gmail.com" href="https://github.com/nandadhrm">
        Ananda Dharma
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="mdpi" height="57" platform="android" src="www/img/logo.png" width="57" />
</widget>
